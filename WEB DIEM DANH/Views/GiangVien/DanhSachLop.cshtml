@model IEnumerable<WEB_DIEM_DANH.Models.SINHVIEN>
@{
    ViewBag.Title = "DanhSachLop";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<table id="example" class="table table-striped table-bordered nowrap" style="width:100%">
    <thead>
        <tr>
            <th>STT</th>
            <th>MSSV</th>
            <th>Họ tên</th>
            <th>Ngày sinh</th>
            <th>Email</th>
            <th>SĐT</th>
        </tr>
    </thead>
    <tbody>
        <tr>         
            @foreach (var item in Model)
            {
                <tr>
                <td>@Html.Raw(item.IDSINHVIEN) </td>
                <td>@Html.Raw(item.MASOSINHVIEN) </td>
                <td>@Html.Raw(item.TENSINHVIEN) </td>
                <td>@Html.Raw(item.NGAYSINH) </td>
                <td>@Html.Raw(item.EMAIL) </td>
                <td>@Html.Raw(item.SODIENTHOAI) </td>   
                </tr>
            }
        </tr>
    </tbody>
</table>

